package com.sanxia.oa.bean;

import java.sql.Date;

/**
 *作者：杨 赢
 *时间：2018年2月21日
 *作用:系统公告实体类
 */
public class Announce {
	//公告Id
	private int id =1;
	//发布人
	private String publish;
	//发布时间
	private Date date;
	//公告标题
	private String title;
	//公告内容
	private String content;
	//部门ID
	private int deptId;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getPublish() {
		return publish;
	}
	public void setPublish(String publish) {
		this.publish = publish;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public int getDeptId() {
		return deptId;
	}
	public void setDeptId(int deptId) {
		this.deptId = deptId;
	}
	
	public Announce(String publish, String title, String content, int deptId) {
		super();
		this.publish = publish;
		this.title = title;
		this.content = content;
		this.deptId = deptId;
	}
	public Announce() {
		super();
	}
	@Override
	public String toString() {
		return "Announce [id=" + id + ", publish=" + publish + ", date=" + date
				+ ", title=" + title + ", content=" + content + ", deptId="
				+ deptId + "]";
	}
	
}
